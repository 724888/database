{
    "name": "Itxiao6/database",
    "description": "The Minkernel Database package.",
    "license": "MIT",
    "homepage": "https://www.itxiao6.top",
    "keywords": ["Minkernel", "database", "sql", "orm"],
    "authors": [
        {
            "name": "itxiao6",
            "email": "itxiao6@qq.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/container": "5.4.*",
        "illuminate/contracts": "5.4.*",
        "illuminate/support": "5.4.*",
        "nesbot/carbon": "~1.20"
    },
    "autoload": {
        "psr-4": {
            "Itxiao6\\Database\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.4-dev"
        }
    },
    "suggest": {
        "doctrine/dbal": "Required to rename columns and drop SQLite columns (~2.5).",
        "fzaninotto/faker": "Required to use the eloquent factory builder (~1.4).",
        "illuminate/console": "Required to use the database commands (5.4.*).",
        "illuminate/events": "Required to use the observers with Eloquent (5.4.*).",
        "illuminate/filesystem": "Required to use the migrations (5.4.*).",
        "illuminate/pagination": "Required to paginate the result set (5.4.*)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
